package task.two.level.one;

import java.util.Scanner;

public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Введите номер квартиры");
		int ap = sc.nextInt();

		if (1 <= ap && ap <= 36) {
			System.out.println("Первый подъезд");
		} else if (36 < ap && ap <= 72) {
			System.out.println("Второй подъезд");
		} else if (72 < ap && ap <= 108) {
			System.out.println("Третий подъезд");
		} else if (108 < ap && ap <= 144) {
			System.out.println("Четвертый подъезд");
		} else {
			System.out.println("Такой квартиры в данном доме не существует");
		}

		int floor = (ap - 1) % 36 / 4;
		floor = floor + 1;

		System.out.println("Этаж номер " + floor);
	}

}
