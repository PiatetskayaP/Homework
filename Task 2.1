package task.two.level.one;

import java.util.Scanner;

public class Runner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Введите номер квартиры");
		int ap = sc.nextInt();

		if (144 < ap || ap < 1) {
			System.out.println("Такой квартиры в данном доме не существует");
		} else {

			int floor = (ap - 1) % 36 / 4;
			floor = floor + 1;

			System.out.println("Этаж номер " + floor);

			int entrance = (ap - 1) / 36;
			entrance = entrance + 1;

			System.out.println("Подъезд номер " + entrance);

		}

	}
}
